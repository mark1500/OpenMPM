FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      M3;
}
// ************************************************************************* //

dimensions          [-1 0 0 0 0 0 0]; //M3=realM3/rho

internalField       uniform 0.0;

boundaryField
{
    walls
    {
        type                zeroGradient;
    }
}

